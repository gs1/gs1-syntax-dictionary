# No commitlint configuration file
VALIDATE_GIT_COMMITLINT=false

# Biome conflicts with ESLint and Prettier (Super Linter warns)
VALIDATE_BIOME_FORMAT=false
VALIDATE_BIOME_LINT=false

# Project uses tab indent and K&R braces; covered by own CI
VALIDATE_CLANG_FORMAT=false
VALIDATE_CPP=false

# Black conflicts with Ruff Format (Super Linter warns)
VALIDATE_PYTHON_BLACK=false

# Prettier conflicts with existing formatters for these types
VALIDATE_HTML_PRETTIER=false
VALIDATE_JSON_PRETTIER=false
VALIDATE_MARKDOWN_PRETTIER=false
VALIDATE_YAML_PRETTIER=false
